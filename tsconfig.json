{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"strictNullChecks": true,
		"allowJs": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"styled-system/*": ["styled-system/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"include": ["styled-system", "src"]
}
